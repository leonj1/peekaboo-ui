server {
        listen 80 default_server;
        listen [::]:80 default_server;

        root /var/www/html;

        index index.html index.htm index.nginx-debian.html;

        server_name _;

        access_log      /var/log/nginx/default_access.log;
        error_log       /var/log/nginx/default_error.log;

        location / {
                try_files $uri $uri/ =404;
        }
}

